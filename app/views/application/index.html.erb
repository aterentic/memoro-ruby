<% provide(:title, 'Home') %>
<h1>Memoro</h1>
<p>The home of your second brain.</p>

<%= form_for(:session, url: sessions_path) do |f| %>
  <%= f.label :email %>
	<%= f.text_field :email %>

  <%= f.label :password %>
	<%= f.password_field :password %>

  <%= f.submit "Sign in", class: "btn btn-medium btn-primary" %>
<% end %>

<p>New user? <%= link_to "Sign up now!", signup_path %></p>
